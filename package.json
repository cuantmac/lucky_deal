    "web": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:staging": "node -r dotenv/config ./node_modules/react-app-rewired/bin/index.js build dotenv_config_path=.env.staging",
    "upload:dev": "node ./scripts/deploy.js -e DEV",
    "upload:staging": "node ./scripts/deploy.js -e STAGING",
    "upload:prod": "node ./scripts/deploy.js -e PRODUCTION",
    "deploy:dev": "yarn build:staging && yarn upload:dev",
    "deploy:staging": "yarn build:staging && yarn upload:staging",
    "deploy:prod": "yarn build && yarn upload:prod",
    "android": "react-native run-android --variant=devDebug",
    "devRelease": "react-native run-android --variant=devRelease",
    "gp": "react-native run-android --variant=gpDebug",
    "gpRelease": "react-native run-android --variant=gpRelease",
    "ios": "react-native run-ios --verbose",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "node scripts/fix.js && jetify",
    "bundle:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "bundle:android": "react-native bundle --entry-file='index.js' --bundle-output='./android/app/src/main/assets/main.jsbundle' --dev=false --platform='android'",
    "img:test": "node ./scripts/hot-update.js -e TEST -p ./ --onlyUploadImage",
    "img:dev": "node ./scripts/hot-update.js -e DEV -p ./ --onlyUploadImage",
    "img:release": "node ./scripts/hot-update.js -e RELEASE -p ./ --onlyUploadImage"
  },
